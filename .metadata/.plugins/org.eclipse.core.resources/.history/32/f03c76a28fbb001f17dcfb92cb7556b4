package Day2;

public class TwoDArray {

	public static void main(java.lang.String[] args) {
		//int a [][] = new int [3][3];
		int a [][]= {	{100,200,300},
						{400,600,900},
						{500,800,700}
						};
		System.out.println("Length of rows is " + a.length);
		System.out.println("Length of columns is " + a[0].length);
		System.out.println(a[2][1]);
	}
	

}
