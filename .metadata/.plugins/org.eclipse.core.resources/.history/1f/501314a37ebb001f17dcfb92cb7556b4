package Day1;

public class Loop {

	public static void main(String[] args) {
	/*
	 
	 while(condition)
	 {
	 statement;
	 inc/dec;
	 }
	 */
        int i=1,j=1;
        while(i<11) {
        	System.out.println(i);
        	i++;
        }
        while(j<=10){
        	System.out.println("Hello");
        	j++;
        }
        while(j<=10) {
        	System.out.println(j);
        	j=j+2;
        }
	}

}
