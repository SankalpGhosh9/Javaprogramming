package Day1;

public class Switchcase {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String a = "Monday";
       switch (a) {
       case "a":System.out.println("Sunday");
       break;
       case "b":System.out.println("Monday is present");;
       break;
       default:System.out.println("day");;
       break;
       }
	}

}
