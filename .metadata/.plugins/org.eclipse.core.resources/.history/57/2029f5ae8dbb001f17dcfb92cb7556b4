package Day2;

public class TwoDArray {

	public static void main(String[] args) {
		//int a [][] = new int [3][3];
		int a [][]= {	{100,200,300},
						{400,200,300},
						{500,200,300}
						};
		System.out.println("Length of rows" + a.length);
		System.out.println("Length of columns" + a[0].length);
	}
	

}
